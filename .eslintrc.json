{
    "parser": "@babel/eslint-parser",
    "plugins": ["flowtype"],
  "extends": ["plugin:flowtype/recommended"],
    // "extends": ["react-app", "eslint:recommended", "plugin:react/recommended", "plugin:react-hooks/recommended"],
    "rules": {
      "no-unused-vars": "warn",
      "eqeqeq": "warn",
      "react-hooks/exhaustive-deps": "warn",
      "no-dupe-keys": "warn"
    },
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  }
  